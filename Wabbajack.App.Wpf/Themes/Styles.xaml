    <Style TargetType="{x:Type wj:FileTreeViewItem}" BasedOn="{StaticResource TreeViewItem}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TreeViewItem}">
                    <ControlTemplate.Resources>
                        <darkBlendTheme:LeftMarginMultiplierConverter x:Key="LengthConverter" Length="16" />
                    </ControlTemplate.Resources>
                    <StackPanel Margin="4, 0, 0, 0">
                        <Border
                            Name="Bd"
                            Padding="{TemplateBinding Padding}"
                            Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"
                            CornerRadius="4"
                            Margin="0, 1, 0, 0">
                            <Grid Margin="{Binding Converter={StaticResource LengthConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="16" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <ToggleButton Grid.Column="0"
                                    x:Name="Expander"
                                    ClickMode="Press"
                                    IsChecked="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}}"
                                    Style="{DynamicResource TreeViewToggleButtonStyle}"
                                    Foreground="{StaticResource PrimaryBrush}"/>

                                <Grid Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="400" />
                                    </Grid.ColumnDefinitions>
                                    <ic:SymbolIcon x:Name="Foreground" IconVariant="Filled" Symbol="{Binding Path=Header.Symbol, RelativeSource={RelativeSource TemplatedParent}}" Margin="0, 0, 4, 0" Foreground="{StaticResource PrimaryBrush}"/>
                                    <TextBlock x:Name="PART_Header" Grid.Column="1" Text="{Binding Path=Header, RelativeSource={RelativeSource TemplatedParent}}" TextTrimming="CharacterEllipsis" Margin="4, 0, 4, 0" VerticalAlignment="Center" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" />
                                    <!--<ComboBox Name="CompilerFileStateListbox" ItemsSource="{wj:EnumToItemsSource {x:Type wj:CompilerFileState}}" DisplayMemberPath="DisplayName" SelectedValue="{Binding Path=Header.CompilerFileState, RelativeSource={RelativeSource TemplatedParent}}" SelectedValuePath="Value" Grid.Column="2" BorderThickness="1" BorderBrush="{StaticResource PrimaryBrush}"/>-->
                                    <sdl:MultiSelectComboBox
                                        Name="CompilerFileStateListbox"
                                        Margin="0, -8, -4, -8"
                                        Height="32"
                                        IsEditable="True"
                                        ItemsSource="{wj:EnumToItemsSource {x:Type wj:CompilerFileState}}"
                                        SelectedItems="{Binding Header.CompilerFileStates, RelativeSource={RelativeSource TemplatedParent}}"
                                        Grid.Column="2"
                                        Background="Transparent"
                                        SetFocusOnFirstSelectedItemOnDropDown="False"
                                        EnableAutoComplete="False">
                                        <sdl:MultiSelectComboBox.Resources>
                                            <SolidColorBrush x:Key="MultiSelectComboBox.DropDown.Button.Background" Color="{StaticResource ComplementaryPrimary16}" />
                                        </sdl:MultiSelectComboBox.Resources>
                                    </sdl:MultiSelectComboBox>
                                </Grid>
                            </Grid>
                        </Border>
                        <ItemsPresenter x:Name="ItemsHost" />
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <DataTrigger Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource DiagonalStripeBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource InverseDiagonalStripeBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalStripeBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource HorizontalStripeBrush}" />
                        </DataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.CompilerFileStates.Count, RelativeSource={RelativeSource Self}}" Value="0" />
                                <Condition Binding="{Binding IsMouseOver, ElementName=Bd}" Value="False" />
                            </MultiDataTrigger.Conditions>
                            <MultiDataTrigger.Setters>
                                <Setter TargetName="CompilerFileStateListbox" Property="Visibility" Value="Collapsed" />
                            </MultiDataTrigger.Setters>
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalHorizontalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalDiagonalStripeBrush}" />
                        </MultiDataTrigger>

